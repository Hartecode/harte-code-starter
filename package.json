{
  "name": "harte-code-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "static-build": "next build && next export",
    "serve": "npx serve out/",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "storybook": "start-storybook -p 6006 public --no-dll",
    "start-storybook": "start-storybook -s ./public -p 9001",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^1.3.3",
    "@chakra-ui/theme": "^1.6.2",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "dotenv": "^8.2.0",
    "framer-motion": "^3.3.0",
    "next": "10.0.6",
    "next-seo": "^4.19.0",
    "next-tinacms-github": "^0.37.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "^4.2.0",
    "react-markdown": "^5.0.3",
    "react-tinacms-github": "^0.37.0",
    "sass": "^1.32.7",
    "styled-components": "^5.2.1",
    "tinacms": "^0.37.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-loader": "^8.2.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^5.0.9",
    "hygen-next-tinacms-github": "https://github.com/dwalkr/hygen-next-tinacms-github",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "stylelint": "^13.10.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "typescript": "^4.1.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all",
    "IE 11"
  ]
}
